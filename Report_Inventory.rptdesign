            <xml-property name="queryText"><![CDATA[SELECT
  iy.inventory_code,
  iy.inventory_name,
  '-' AS satuan,
  (
  SELECT
    SUM( askl.qty ) AS stok_keluar 
  FROM
    adjustment_stock ask
    LEFT JOIN adjustment_stock_detail askl ON askl.adjustment_stock_id = ask.adjustment_stock_id 
  WHERE
    ask.adjustment_type_code = 'out' 
    AND askl.inventory_id = ivi.inventory_id 
  ) AS stok_keluar,
  (
  SELECT
    SUM( askl.qty ) AS stok_keluar 
  FROM
    adjustment_stock ask
    LEFT JOIN adjustment_stock_detail askl ON askl.adjustment_stock_id = ask.adjustment_stock_id 
  WHERE
    ask.adjustment_type_code = 'in' 
    AND askl.inventory_id = ivi.inventory_id 
  ) AS stok_masuk 
FROM
  inventory_item ivi
  LEFT JOIN inventory iy ON iy.inventory_id = ivi.inventory_id 
WHERE
  (ivi.item_id = ? OR ? IS NULL)
  AND ivi.created_dttm >= ? 
  AND ivi.created_dttm <= ? 
GROUP BY
  ivi.inventory_id;]]></xml-property>
